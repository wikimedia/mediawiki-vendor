{
    "name": "gr4vy/gr4vy-php",
    "description": "Welcome to the Gr4vy PHP SDK",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://openapi-generator.tech",
    "license": "MIT",
    "type":"library",
    "authors": [
        {
            "name": "Gr4vy Inc",
            "homepage": "https://gr4vy.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": ">=6.5.5",
        "lcobucci/jwt": "^4.1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "friendsofphp/php-cs-fixer": "^3.4.0"
    },
    "autoload": {
        "psr-4": {
            "Gr4vy\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Gr4vy\\Test\\" : "test/" }
    }
}
