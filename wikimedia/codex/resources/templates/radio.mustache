<div class="cdx-radio{{!
  }}{{#isInline}} cdx-radio--inline{{/isInline}}{{#renderClasses}}{{wrapperAttributes}}{{/renderClasses}}"{{!
  }}{{#wrapperId}} id="{{ wrapperId }}"{{/wrapperId}}{{!
  }}{{#renderAttributes}}{{{ wrapperAttributes }}}{{/renderAttributes}}>
  <input class="cdx-radio__input{{#renderClasses}}{{inputAttributes}}{{/renderClasses}}" type="radio"{{#inputId}} id="{{ inputId }}"{{/inputId}} name="{{name}}" value="{{value}}"{{!
    }}{{#isChecked}} checked{{/isChecked}}{{!
    }}{{#isDisabled}} disabled{{/isDisabled}}{{!
    }}{{#ariaDescribedby}} aria-describedby="{{ ariaDescribedby }}"{{/ariaDescribedby}}{{!
    }}{{#renderAttributes}}{{{ inputAttributes }}}{{/renderAttributes}}>
  <span class="cdx-radio__icon"></span>
  {{#label}}
    {{> label}}
  {{/label}}
</div>
