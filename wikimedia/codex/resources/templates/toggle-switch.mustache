<span class="cdx-toggle-switch{{#renderClasses}}{{wrapperAttributes}}{{/renderClasses}}"{{!
  }}{{#wrapperId}} id="{{ wrapperId }}"{{/wrapperId}}{{!
  }}{{#renderAttributes}}{{{ wrapperAttributes }}}{{/renderAttributes}}>
  <input class="cdx-toggle-switch__input{{#renderClasses}}{{inputAttributes}}{{/renderClasses}}" type="checkbox"{{!
    }}{{#inputId}} id="{{ inputId }}"{{/inputId}}{{#name }} name="{{ name }}"{{/name}}{{#value}} value="{{ value }}"{{/value}}{{!
    }}{{#isChecked}} checked{{/isChecked}}{{!
    }}{{#isDisabled}} disabled{{/isDisabled}}{{!
    }}{{#ariaDescribedby}} aria-describedby="{{ ariaDescribedby }}"{{/ariaDescribedby}}{{!
    }}{{#renderAttributes}}{{{ inputAttributes }}}{{/renderAttributes}}>
  <span class="cdx-toggle-switch__switch">
    <span class="cdx-toggle-switch__switch__grip"></span>
  </span>
    {{#label}}
        {{> label}}
    {{/label}}
</span>
