<{{#isLegend}}legend{{/isLegend}}{{^isLegend}}div{{/isLegend}} class="{{#coreClass}}{{ coreClass }} {{/coreClass}}cdx-label{{!
  }}{{#isVisuallyHidden}} cdx-label--visually-hidden{{/isVisuallyHidden}}{{!
  }}{{#isDisabled}} cdx-label--disabled{{/isDisabled}}{{#renderClasses}}{{{attributes}}}{{/renderClasses}}"{{!
  }}{{#id}} id="{{ id }}"{{/id}}{{!
  }}{{#renderAttributes}}{{{attributes}}}{{/renderAttributes}}>
  {{#isLegend}}
    <span class="cdx-label__label">
      {{#icon}}<span class="cdx-label__label__icon {{ icon }}"></span>{{/icon}}{{!
      }}<span class="cdx-label__label__text">{{ labelText }}</span>{{!
      }}{{#optionalFlag}} <span class="cdx-label__label__optional-flag">{{#i18n}}cdx-label-optional-flag{{/i18n}}</span>{{/optionalFlag}}
    </span>
  {{/isLegend}}
  {{^isLegend}}
    <label {{#inputId}}for="{{ inputId }}" {{/inputId}}class="cdx-label__label">
      {{#icon}}<span class="cdx-label__label__icon {{ icon }}"></span>{{/icon}}{{!
      }}<span class="cdx-label__label__text">{{ labelText }}</span>{{!
      }}{{#optionalFlag}} <span class="cdx-label__label__optional-flag">{{#i18n}}cdx-label-optional-flag{{/i18n}}</span>{{/optionalFlag}}
    </label>
  {{/isLegend}}
  {{#description}}
    <span {{#descriptionId}}id="{{ descriptionId }}" {{/descriptionId}}class="cdx-label__description">{{{ description }}}</span>
  {{/description}}
</{{#isLegend}}legend{{/isLegend}}{{^isLegend}}div{{/isLegend}}>
